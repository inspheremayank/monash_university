{% set network = _Network.getNetworkData() %}
{% set blog = _Blog.getBlog() %}

<!doctype html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang=""> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8" lang=""> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9" lang=""> <![endif]-->
<!--[if gt IE 8]><!-->
{{ this.beginPage() }}
<html class="no-js" lang="en-US">
    <!--<![endif]-->

    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>{{this.title | raw}}</title>
        <meta name="description" content="A free html template with Sketch design made with Bootstrap">
        <meta name="keywords" content="free html template, bootstrap, ui kit, sass" />
        <meta name="author" content="Codrops" />
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <!-- favicon generated by http://realfavicongenerator.net/ -->
        {% set favicon = network.templatePath~'/static/images/favicon/favicon-96x96.png' %}
        {% if network.favMedia | length > 0 %}
            {% set favicon = network.favMedia['path'] %}
        {% endif %}         
        <link rel="icon" type="image/png" href="{{favicon}}">
        <!-- end favicon links -->
        <link rel="stylesheet" href="{{network.templatePath}}/static/css/bootstrap.min.css" />
        <link rel="stylesheet" href="{{network.templatePath}}/static/css/normalize.min.css">
        <link rel="stylesheet" href="{{network.templatePath}}/static/css/animate.min.css">
        <link rel="stylesheet" href="{{network.templatePath}}/static/css/flickity.min.css">
        <link rel="stylesheet" href="{{network.templatePath}}/static/css/styles.css">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
        {{ this.head() }}
    </head>

    <body>
        {{ this.beginBody() }}
        {{ this.render('partials/_header.twig', {network:network, blog:blog}) | raw }}
        
        
        {{ this.render('partials/_javascript.twig', {network: network}) | raw }}
        {{ this.endBody() }}
    </body>
</html>
{{ this.endPage() }}